{
    "name": "IBMcloudSpaceManagement",
    "version": "0.0.1",
    "classes": [
        {
            "class": "bluemix.space.get",
            "description": "Show current Bluemix space",
            "emittarget": "bluemix.space.get",
            "texts": [
                "What is the current space?",
                "What is my space?",
                "Which space I am in?",
                "Tell me my current Bluemix space"
            ]
        },
        {
            "class": "bluemix.space.set",
            "description": "Set the current Bluemix space",
            "emittarget": "bluemix.space.set",
            "texts": [
                "change space",
                "use space",
                "switch to space",
                "use another Bluemix space"
            ],
            "parameters" : [
			   {
			       "name": "spacename",
			       "type": "entity",
			       "prompt": "OK. What is the name of the space you want to use?"
			   }
	       ]
        },
        {
            "class": "bluemix.space.list",
            "description": "List Bluemix spaces",
            "emittarget": "bluemix.space.list",
            "texts": [
                "What are the spaces?",
                "List all spaces",
                "tell me my Bluemix spaces"
            ]
        },
        {
            "class": "bluemix.space.help",
            "description": "Help with Bluemix spaces",
            "emittarget": "bluemix.space.help",
            "texts": [
                "Can you help me with spaces?",
                "How do I switch Bluemix spaces?"
            ]
        }
    ]
}
