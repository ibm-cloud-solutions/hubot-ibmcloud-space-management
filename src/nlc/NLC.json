{
    "name": "IBMcloudSpaceManagement",
    "version": "0.0.1",
    "classes": [
        {
            "class": "bluemix.space.get",
            "emittarget": "bluemix.space.get",
            "texts": [
                "What is the current space?",
                "What is my space?",
                "Which space I am in?",
                "Tell me my current space"
            ]
        },
        {
            "class": "bluemix.space.set",
            "emittarget": "bluemix.space.set",
            "texts": [
                "change space",
                "use space",
                "swith to space",
                "I want to swith spaces",
                "I want to use another space"
            ],
            "parameters" : [
			   {
			       "name": "spacename",
			       "type": "entity",
			       "prompt": "OK. What is the name of the space you want to use?"
			   }
	       ]
        },
        {
            "class": "bluemix.space.list",
            "emittarget": "bluemix.space.list",
            "texts": [
                "What are the spaces?",
                "List all spaces",
                "tell me my spaces"
            ]
        },
        {
            "class": "bluemix.space.help",
            "emittarget": "bluemix.space.help",
            "texts": [
                "Can you help me with spaces?",
                "How do I switch spaces?"
            ]
        }
    ]
}
